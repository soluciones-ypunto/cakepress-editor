{
    "name": "xavier83ar/cakepress-editor",
    "description": "Cakepress Editor plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "zendframework/zend-json": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakepressEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakepressEditor\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
